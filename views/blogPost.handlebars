<html lang="en">
  <head>
    <title>Blog Home Page</title>
  </head>
  <body>
    <div class="container">
      {{#each blogs}}
        <div class="blog">
          <a href="/blogpost/{{this.slug}}">
            <h2> {{this.title}} </h2>
          </a>
        </div>
      {{/each}}
    </div>
  </body>
</html>